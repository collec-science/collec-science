<table name="barcode" layers="0" collapse-mode="2" max-obj-count="3" z-value="0">
	<schema name="col"/>
	<role name="collec"/>
	<comment><![CDATA[Models of barcodes usable]]></comment>
	<tag name="container"/>
	<position x="1620" y="1800"/>
	<column name="barcode_id" not-null="true">
		<type name="serial" length="0"/>
	</column>
	<column name="barcode_name" not-null="true">
		<type name="varchar" length="0"/>
		<comment><![CDATA[Name of the model]]></comment>
	</column>
	<column name="barcode_code" not-null="true" default-value="'QR'">
		<type name="varchar" length="0"/>
		<comment><![CDATA[Value of the barcode used by the generating application, if occures. Default value: QR for QRcode]]></comment>
	</column>
	<constraint name="barcode_type_pk" type="pk-constr" table="col.barcode">
		<columns names="barcode_id" ref-type="src-columns"/>
	</constraint>
	<initial-data>
<![CDATA[barcode_name•barcode_code⸣
QRCode•QR⸣
EAN 128•C128]]>
	</initial-data>
</table>

